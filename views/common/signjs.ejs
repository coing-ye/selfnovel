<script>
    link = document.location.href;
    var very = link.split('/');
    console.log(very[very.length-1]);
    var x = document.getElementById("login");
    var y = document.getElementById("register");
    var z = document.getElementById("signbtn");

    if (very[very.length-1] == "signin"){
        var url = "https://image.freepik.com/free-vector/code-typing-concept-illustration_114360-3581.jpg";
        $(".login").css("background-image", `url(${url})`);
    }
    else if(very[very.length-1] == "signup"){
        var url = "https://www.top-tier.co.kr/mobile/img/sub/community/title_img.png";
        $(".login").css("background-image", `url(${url})`);
        $("#login").css('left','3000px');
        $("#register").css('left','50px');
        $("#signbtn").css('left','125px');

    } 
    function login(){
        x.style.left = "50px";
        y.style.left = "450px";
        z.style.left = "0";
    }
    function register(){
        x.style.left = "-400px";
        y.style.left = "50px";
        z.style.left = "125px";
    }
</script>